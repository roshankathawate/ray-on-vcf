# Example environment variables for Carvel package build
# Copy this file to .env and update with your actual values
# Then run: source .env

# Required Variables
export CARVEL_PACKAGE_TAG=0.0.2
export CARVEL_PACKAGE_VERSION=0.0.2
export DOCKER_REGISTRY_USER_NAME=user01
export DOCKER_REGISTRY_PASSWORD="your-password"
export DOCKER_ARTIFACTORY_URL=your-docker-registry.example.com
export CARVEL_BUNDLE_REGISTRY_URL=your-docker-registry.example.com/carvel/your-project

# Docker Base Image Configuration
# For corporate environments with Docker proxy:
export DOCKER_BASE_IMAGE=dockerhub-proxy.your-registry.example.com/library/ubuntu:22.04
# For public Docker Hub (comment above and uncomment below):
# export DOCKER_BASE_IMAGE=ubuntu:22.04

# Optional Variables (required for upload to repository)
export CARVEL_PACKAGE_REPOSITORY_URL=https://your-artifactory-url/artifactory
export PACKAGE_TYPE=your_package_type
